{
    "name": "stdgroup/laravel-generator",
    "type": "library",
    "description": "Install Laravel, generate base structure",
    "keywords": [
        "stdgroup",
        "laravel-generator"
    ],
    "homepage": "https://github.com/stdgroup/laravel-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Tuan Pham",
            "homepage": "https://github.com/stdgroup",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/console": "^4.3",
        "symfony/process": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "StdGroup\\LaravelGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StdGroup\\LaravelGenerator\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
